{
  "name": "@exchanges/source",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=20.8.0"
  },
  "scripts": {
    "prod:api": "node ./dist/apps/api/main.js",
    "prod:ticker": "node ./dist/apps/binance-ticker/main.js",
    "start:api": "TZ='UTC' npx nx run-many --target=serve --projects=api --configuration=development",
    "start:binance-ticker": "TZ='UTC' npx nx run-many --target=serve --projects=binance-ticker --configuration=development",
    "start:api-ticker": "TZ='UTC' node --trace-warnings node_modules/.bin/nx run-many --target=serve --projects=api,binance-ticker --configuration=development",
    "build": "npx nx run-many --target=build --all --parallel --configuration=production --prod --no-cache --skip-nx-cache",
    "prisma:init": "nx run-many -t migrate",
    "prisma:generate": "nx run-many -t generate-types",
    "prisma:db-pull": "nx run-many -t db-pull",
    "docker:run": "docker compose --env-file .env -p cluster -f tools/scripts/docker-compose.yml up -d --remove-orphans"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^7.1.14",
    "@nx/js": "17.0.2",
    "@nx/nest": "17.0.2",
    "@nx/webpack": "17.0.2",
    "@nx/node": "17.0.2",
    "@nx/workspace": "17.0.2",
    "@prisma/client": "^5.5.2",
    "@types/pako": "^2.0.2",
    "@types/websocket": "^1.0.8",
    "ccxt": "^4.4.26",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "pako": "^2.1.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "17.0.2",
    "@nx/eslint-plugin": "17.0.2",
    "@nx/jest": "17.0.2",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "17.0.2",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "supabase": "^1.110.1",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  }
}
